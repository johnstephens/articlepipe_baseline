<h1>Listing profiles</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Display name</th>
      <th>Tagline</th>
      <th>Bio</th>
      <th>Twitter</th>
      <th>Personal url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.display_name %></td>
        <td><%= profile.tagline %></td>
        <td><%= profile.bio %></td>
        <td><%= profile.twitter %></td>
        <td><%= profile.personal_url %></td>
        <td><%= link_to 'Show', profile %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

